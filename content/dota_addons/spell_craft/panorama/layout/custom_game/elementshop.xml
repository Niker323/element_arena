<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/custom_game/elementshop.css" />
	</styles>
	<scripts>
		<include src="file://{resources}/scripts/custom_game/elementshop.js" />
	</scripts>
    
	<Panel hittest="false" class="ElementShopPanel">
        <Image hittest="false" id="BG_Image" src="file://{images}/custom_game/all/leftpanel.png"/>
        <Panel hittest="false" id="ShopInfo" class="ShopInfoAnim">
            <Image hittest="false" id="buttonBG2_Image" src="file://{images}/custom_game/all/elementshopinfo.png"/>
            <Image hittest="false" src="file://{images}/custom_game/all/st2.png" style="height:400px;width:10px; margin-left:260px;"/>
            <Image hittest="false" src="file://{images}/custom_game/all/st2.png" style="height:400px;width:10px; margin-left:530px;"/>
            <Button id="ToggleX3mode" onactivate='ToggleX3mode()' style="height:20px;width:20px; margin-top:10px; margin-left:800px;" onmouseover='DOTAShowTextTooltip(x3 Mode)' onmouseout='DOTAHideTextTooltip()'>
                <Image hittest="false" src="file://{images}/custom_game/all/x3.png" style="height:30px;width:30px;"/>
			</Button>
            <Button id="OffInfo" onactivate='OffInfo()' style="height:100px;width:20px; margin-top:150px; margin-left:800px;">
                <Image hittest="false" src="file://{images}/custom_game/all/st5.png" style="height:100px;width:20px;"/>
			</Button>
