<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/custom_game/endswaves.css" />
	</styles>
	<scripts>
		<include src="file://{resources}/scripts/custom_game/endswaves.js" />
	</scripts>
    
	<Panel hittest="false" class="endswavesPanel">
		<Panel hittest="false" id="endswaves">
			<Image hittest="false" id="BG_Image" src="file://{images}/custom_game/all/endswavesfon.png"/>
			<Label hittest="false" id="question" text="#endswaves"/>
